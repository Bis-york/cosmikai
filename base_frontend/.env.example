# Copy to `.env` and fill values.

# --- Required service URLs ---
VITE_API_BASE_URL=
VITE_VISUAL_BASE_URL=

# --- Vite dev server ---
VITE_HOST=0.0.0.0
VITE_PORT=5180
VITE_STRICT_PORT=true
VITE_HTTPS=false
VITE_OPEN=false

# --- Vite preview server ---
VITE_PREVIEW_HOST=0.0.0.0
VITE_PREVIEW_PORT=5280

# --- CORS for Vite dev server (optional) ---
VITE_ALLOWED_ORIGINS=

# --- Base path if served behind a subpath (optional) ---
VITE_BASE=/
